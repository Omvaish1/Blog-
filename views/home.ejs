<%- include("./partials/header") -%>

<h1>Home</h1>
<p><%= startContent%></p>
<%posts.forEach(function(post){%>
   <h1><%=post.title%></h1>
   <%const slicedText = post.content.slice(0,100) + "...";%> 
   <p><%=slicedText%> <a href="/posts/<%=post.title%>">Read More</a></p>
<%})%>

<%- include("./partials/footer") -%>
